module IdReturnNullabilityAliasSignature;

fn chooseMaybe(flag: bool, when_true: id, when_false: id) -> id? {
  if (flag) {
    return when_true;
  }
  return when_false;
}

fn chooseAlways(flag: bool, when_true: id, when_false: id) -> id! {
  if (flag) {
    return when_true;
  }
  return when_false;
}

fn main() -> i32 {
  return chooseMaybe(true, 30, 2) + chooseAlways(false, 5, 7);
}
