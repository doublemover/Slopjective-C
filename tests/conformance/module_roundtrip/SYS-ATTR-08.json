  "source": "// RUN: objc3c -fobjc-version=3 --emit-module SYSATTR08.objcmod --emit-interface SYSATTR08.obji %s\n// RUN: objc3c --verify-interface SYSATTR08.objcmod SYSATTR08.obji %s\n#define SYS_BORROWED_PTR borrowed const char *\n#define SYS_RETURNS_BORROWED(N) __attribute__((objc_returns_borrowed(owner_index=N)))\nSYS_BORROWED_PTR borrow_slice(const char *owner, unsigned long offset) SYS_RETURNS_BORROWED(0);\n",
  "expect": {
    "parse": "accept",
    "diagnostics": [],
    "canonical_interface": "borrowed const char *borrow_slice(const char *owner, unsigned long offset) __attribute__((objc_returns_borrowed(owner_index=0)));\n",
    "canonicalization": {
      "normalizes_noncanonical_input": true,
      "normalization_pairs": [
        {
          "from": "SYS_BORROWED_PTR",
          "to": "borrowed const char *"
        },
        {
          "from": "SYS_RETURNS_BORROWED(0)",
          "to": "__attribute__((objc_returns_borrowed(owner_index=0)))"
        }
      ],
      "forbidden_noncanonical_spellings": [
        "SYS_BORROWED_PTR",
        "SYS_RETURNS_BORROWED(0)"
      ]
    },
    "metadata": {
      "preserved_fields": [
